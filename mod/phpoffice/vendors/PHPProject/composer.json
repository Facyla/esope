{
    "name": "phpoffice/phpproject",
    "description": "PHPProject - Read, Create and Write Project Management documents in PHP",
    "keywords": ["PHP","Project","GanttProject","gan","mpx"],
    "homepage": "http://phpoffice.github.io",
    "type": "library",
    "license": "LGPL",
    "version": "0.2.0",
    "authors": [
        {
            "name": "Mark Baker"
        },
        {
            "name": "Franck Lefevre",
            "homepage": "http://rootslabs.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-xml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpdocumentor/phpdocumentor":"2.*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "2.*",
        "phploc/phploc": "2.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-0": {
            "PHPProject": "src/"
        },
        "psr-4": {
            "PhpOffice\\PhpProject\\": "src/PhpProject/"
        }
    }
