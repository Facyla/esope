{
	"type": "project",
	"name": "elgg/starter-project",
	"description": "This is a quickstart skeleton for building a site based on Elgg 3.x.",
	"license": "MIT",
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"elgg/elgg": "3.1.3",
        "php": ">=7.1",
        "ext-pdo": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-xml": "*",
        "elgg/login_as": "~2.0.0",
        "bower-asset/fontawesome": "~5.6",
        "bower-asset/jquery": "^2.1.4",
        "bower-asset/jquery-colorbox": "^1.5.14",
        "bower-asset/jquery-form": "^3.51",
        "bower-asset/jquery-treeview": "^1.4",
        "bower-asset/jquery-ui": "~1.11.4",
        "bower-asset/jquery.imgareaselect": "~0.9.10",
        "bower-asset/normalize-css": "*",
        "bower-asset/requirejs": "^2.1.18",
        "bower-asset/sprintf": "~1.0.3",
        "bower-asset/text": "^2.0.4",
        "ckeditor/ckeditor": "~4.12.0",
        "css-crush/css-crush": "^2.4.0",
        "doctrine/cache": "^1.6",
        "doctrine/dbal": "~2.5",
        "fzaninotto/faker": "^1.6",
        "imagine/imagine": "^0.7.1",
        "league/flysystem": "~1.0",
        "league/flysystem-memory": "^1.0",
        "mrclay/minify": "~2.0",
        "michelf/php-markdown": "^1.5.0",
        "misd/linkify": "~1.1.2",
        "monolog/monolog": "^1.23",
        "npm-asset/cropperjs": "~1.4.3",
        "npm-asset/jquery-cropper": "~1.0.0",
        "npm-asset/formdata-polyfill": "3.0.15",
        "npm-asset/weakmap-polyfill": "^2.0",
        "peppeocchi/php-cron-scheduler": "2.*",
        "php-di/php-di": "^5.4",
        "react/promise": "^2.5",
        "roave/security-advisories": "dev-master",
        "robmorgan/phinx": "^0.11.1",
        "symfony/console": "~3.3",
        "symfony/http-foundation": "~3.4",
        "symfony/polyfill-php71": "^1.7",
        "symfony/polyfill-php72": "^1.7",
        "symfony/routing": "^3.3",
        "symfony/var-dumper": "~3.3",
        "tedivm/stash": "~0.15.0",
        "vanilla/htmlawed": "~2.2.0",
        "zendframework/zend-mail": "~2.4"
	},
	"require-dev": {
		"phpunit/phpunit": "~6.4"
	},
	"scripts": {
		"post-install-cmd": "\\Elgg\\Composer\\PostInstall::execute",
		"post-package-update": "\\Elgg\\Composer\\PostUpdate::execute",
		"test": "phpunit"
	},
	"config": {
		"process-timeout": 0,
		"platform": {
			"php": "7.1"
		},
		"fxp-asset": {
			"enabled": false
		},
		"optimize-autoloader": true,
		"apcu-autoloader": true
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	]
}
